@model CkeckIn.Models.NotFoundPageView

@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="ru">

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>
        @Model.Title
    </title>

    <!-- Bootstrap core CSS -->

    <link href="~/lib/gentelella/production/css/bootstrap.min.css" rel="stylesheet">

    <link href="~/lib/gentelella/production/fonts/css/font-awesome.min.css" rel="stylesheet">
    <link href="~/lib/gentelella/production/css/animate.min.css" rel="stylesheet">

    <!-- Custom styling plus plugins -->
    <link href="~/lib/gentelella/production/css/custom.css" rel="stylesheet">
    <link href="~/lib/gentelella/production/css/icheck/flat/green.css" rel="stylesheet">


    <script src="~/lib/gentelella/production/js/jquery.min.js"></script>

    <!--[if lt IE 9]>
          <script src="../assets/js/ie8-responsive-file-warning.js"></script>
          <![endif]-->
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
            <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
            <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
          <![endif]-->

</head>


<body class="nav-md">
    <div class="container body">
        <div class="main_container">
            <!-- page content -->
            <div class="col-md-12">
                <div class="col-middle">
                    <div class="text-center text-center">
                        <h1 class="error-number">404</h1>
                        <h2>
                            @Model.Title
                        </h2>
                        <p>
                            @Model.Message
                        </p>
                        <div class="mid_center">
                            <form asp-action="@Model.Action" asp-controller="@Model.Controller" method="get">
                                <div class="col-xs-12 form-group pull-right top_search">
                                    <div class="input-group">
                                        <select name="id" class="select2_single form-control" asp-items="@Model.EntitySelectList"></select>
                                        <span class="input-group-btn">
                                            <button class="btn btn-default" type="submit">Выбрать</button>
                                        </span>
                                    </div>
                                </div>
                            </form>
                        </div>
                        <p>
                            <a href="/@Model.ReturnUrl">
                                < Вернуться назад >
                            </a>
                        </p>
                    </div>
                </div>
            </div>
            <!-- /page content -->
        </div>
    </div>

    <div id="custom_notifications" class="custom-notifications dsp_none">
        <ul class="list-unstyled notifications clearfix" data-tabbed_notifications="notif-group"></ul>
        <div class="clearfix"></div>
        <div id="notif-group" class="tabbed_notifications"></div>
    </div>

    <script src="~/lib/gentelella/production/js/bootstrap.min.js"></script>

    <!-- bootstrap progress js -->
    <script src="~/lib/gentelella/production/js/progressbar/bootstrap-progressbar.min.js"></script>
    <!-- icheck -->
    <script src="~/lib/gentelella/production/js/icheck/icheck.min.js"></script>

    <script src="~/lib/gentelella/production/js/custom.js"></script>
    <!-- pace -->
    <script src="~/lib/gentelella/production/js/pace/pace.min.js"></script>
    <!-- /footer content -->
</body>

</html>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
